<h1> New Post </h1>

<%= form_with model: @post do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  </br>
  <div>
    <%= form.label :body %>
    <%= form.text_area :body %>
  </div>
  <div>
    <%= form.label :photo %>
    <%= form.file_field :photo %>
  <div>
  <%= form.submit "Create"%>
<% end %>