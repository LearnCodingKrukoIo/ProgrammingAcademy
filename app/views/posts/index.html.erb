<h1> My Posts </h1>
<% @posts.each do |post| %>
    <h2> <%= post.title%> </h2>
    <%= post.body %>
    <% if post.photo.attached? %>
      <image src="<%= (url_for(post.photo)) %>">
    <% end %>
    <h3> Comments: </h3>
    <% post.comments.each do |comment| %>
      <%= comment.body %> <br /> 
    <%end%>
<%end%>
